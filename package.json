{
  "name": "atlas-online-store",
  "version": "1.0.0",
  "description": "Atlas Online Store - A modern e-commerce platform built with Next.js, TypeScript, and Tailwind CSS",
  "author": "CodeHuddle <harisahmed629@gmail.com>",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "clean": "rimraf .next out coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check-types": "tsc --noEmit --pretty",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "commit": "cz",
    "prepare": "husky",
    "generate:component": "node ./scripts/generate-component.mjs"
